    "express": "^4.17.1",
    "faker": "^4.1.0",
    "graphql": "^14.5.4",
    "graphql-tools": "^4.0.5",
    "graphql-voyager": "^1.0.0-rc.27",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3"
  },
